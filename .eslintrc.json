{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-spacing": 0,
        "new-cap": 0,
        "max-len": ["error", { "code": 100 }],
        "linebreak-style": ["error", "windows"],
        "indent": [ "error", 4]
    }
}
